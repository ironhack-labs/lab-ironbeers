{{!-- using Partials--}}
<h1>Random Beers</h1>
<div class="container">
  <div class="row">
      {{#each randombeers}} 
        {{> beerpartial this}} 
         <p><strong>Food Pairing</strong></p>
    <ul>
      {{#each food_pairing}}
        <li>{{this}}</li>
      {{/each}}
    </ul>
    <p><strong>Brewers Tips:</strong> {{brewers_tips}}</p>
      {{/each}}

      
  </div>
</div>

