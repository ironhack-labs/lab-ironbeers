<div class="">
    {{#each beer}}
    <div class="d-flex flex-column align-items-start">
        {{> beerpartial this}}
        <div>
            <h4>Food Pairing</h4>
            <ul>
                {{#each food_pairing}}
                <li>{{this}}</li>
                {{/each}}
            </ul>
            <p><strong>Brewer Tips:</strong> {{this.brewers_tips}}</p>
        </div>
    </div>
    {{/each}}
</div>