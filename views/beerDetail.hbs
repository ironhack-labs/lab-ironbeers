<div class="container">
    <div class="row d-flex flex-row jusfity-content-center">
        {{#each beer}}
        {{> beerpartial this}}
        <div class="col">
            <h4>Food Pairing</h4>
            <ul>
                {{#each food_pairing}}
                <li>{{this}}</li>
                {{/each}}
            </ul>
            <p><strong>Brewer Tips:</strong> {{this.brewers_tips}}</p>
        </div>
        {{/each}}
    </div>
</div>