<div class="random-beer">
    {{#each responseFromAPI}}
    {{> beerpartial this}}
    <div class="food-pairing">
        <strong>Food pairing:</strong>
        <ul>
            {{#each food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
    </div>
    <div class="brewer-tips">
        <strong>Brewer tips:</strong>
        {{brewers_tips}}
    </div>
    {{/each}}
</div>