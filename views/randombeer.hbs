{{#each beer}}
<div class="single-beer-container">

    <div class="single-beer">
    {{>beerpartial this}}
    <h3>Food pairing: 
        <ul>
            {{#each this.food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
    </h3>
    <h3>Brewer tips: {{brewers_tips}}</h3>
    </div>
</div>
{{/each}}