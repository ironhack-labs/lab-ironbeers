   {{#each beers}}
   <div class="randombeer-wrapper">
        <div>
            <img src="{{image_url}}" >
         </div>
        <div class="randombeer-info">
             <h1>{{name}}</h1>
            <p>{{description}}</p>
            <p>{{tagline}}</p>
            <h3> Food Pairing</h3>
            {{#each food_pairing}}
             <li>{{this}}</li>
            {{/each}}
            <p><b>Brewer tips:</b>{{brewers_tips}}</p> 
        </div>
   </div>
    
    
     
    {{/each}}