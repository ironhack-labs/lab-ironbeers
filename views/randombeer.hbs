<div>
    {{!-- <a href="">{{this.name}}</a> --}}
    {{#each responseFromAPI}}
        <img src="{{this.image_url}}" class="card-img-top" alt="{{this.name}}">
        <h5 class="card-title">{{this.name}}</h5>
        <h6 class="card-subtitle mb-2 text-muted">{{this.tagline}}</h6>
        <p class="card-text">{{this.description}}</p>
        <br>
        <h5>Food Pairing:</h5>
        <ul> 
            {{#each this.food_pairing}}
                <li>
                    {{this}}
                </li>
            {{/each}}
        </ul>
    
        <br>
        <h6>Brewers Tips: {{this.brewers_tips}}</h6>
    
    {{/each}}

</div>