<div>
    {{#each beers}}
    <div class = "beer">
        {{>beerpartial this}}
        <div class = "beer-inner">
            <p>First brewed: {{this.first_brewed}}</p>
            <p>Attenuation level: {{this.attentuation_level}}</p>
            <p>Recommended food pairing:</p>
            <ul>
                {{#each this.food_pairing}}
                <li>{{this}}</li>
                {{/each}}
            </ul>
        </div>
    </div>
    {{/each}}
    <div class = "button">
        <a href="/beers">Go back to beers</a>
    </div>
</div>