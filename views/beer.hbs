<div id="random">
    {{#each beer}}
        <img src="{{this.image_url}}">
        <div>
        <h2>{{this.name}}</h2>
        <p class="tag">{{this.tagline}}</p>
        <p>{{this.description}}</p>
        <h4>Food Pairing</h4>
        <ul>
            {{#each food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <p><b>Brewers Tips:</b> {{this.brewers_tips}}</p>
        </div>
    {{/each}}
</div>