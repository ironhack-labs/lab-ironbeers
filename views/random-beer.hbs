
{{#each responseFromAPI as |beer|}}
    <img src='{{beer.image_url}}' alt="{{beer.name}}">
    <h1>{{beer.name}}</h1>
    <h4>{{beer.tagline}}</h4>
    <p>{{beer.description}}</p>
    <h2>Food pairing</h2>
    <ul>
        {{#each beer.food_pairing as |food|}}
        <li>{{food}}</li>
        {{/each}}
    </ul>
    <p>{{beer.brewer_tips}}</p>
{{/each}}