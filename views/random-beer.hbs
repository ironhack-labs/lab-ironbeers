
<div id="random-box">
    <img src="{{beer.image_url}}" alt="no Image provided">

    <div id="random-text-box">
        <h2>{{beer.name}}</h2>
        <h4>{{beer.tagline}}</h4>
        <p>{{beer.description}}</p>
        <h3>Food pairing</h3>
        <ul>
            {{#each beer.food_pairing}}
                <li>{{this}}</li>
            {{/each}}
        </ul>
        <p><b>Brewers Tips: </b>{{beer.brewers_tips}}</p>
    </div>

</div>


