<h1>The Random Beer List</h1>

<div>
      {{#each beers}}
  <div>
    <img src="{{this.image_url}}" />
    <h2>{{this.name}}</h2>
    <h3>{{this.tagline}}</h3>
    <p>{{this.description}}</p>
    <h3>Food pairing</h3>
    <ul>
      {{#each this.food_pairing}}
        <li>{{this}}</li>
      {{/each}}
    </ul>
      {{/each}}
  </div>
</div>