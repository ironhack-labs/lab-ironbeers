<div id="randomBeerPage">
    <div id="random_img">
        <img src="{{image_url}}" alt="beer img"/>
    </div>
    <div id="randomBeerContent">
        <h1>{{name}}</h1>
        <h3>{{tagline}}</h3>
        <p class="description">{{description}}</p>
        <h2>Food Pairing</h2>
        <ul>
            {{#each food_pairing}} 
                <li>  {{this}}   </li>
            {{/each}}
        </ul>
        <p><em>Brewers Tips : </em> {{brewers_tips}}</p>
    </div>
</div>