<h1>Ranbom Beer</h1>

<ul>

  {{#each beers}}

    <h2>Food Pairing</h2>
    <div>
      <img src='{{this.image_url}}' alt='{{this.name}}' />
    </div>
    <ul>
      {{#each food_pairing}}
        <li>{{this}}</li>
      {{/each}}
    </ul>
    <p>Brewer Tips: {{brewers_tips}}</p>
  {{/each}}
</ul>