{{#each randomBeer}}
    <div class="container">
        <img src={{this.image_url}} />

        <h1>{{this.name}}</h1>

        <p>{{this.description}}</p>

        <p>{{this.tagline}}</p>

        <h2>Food Pairing</h2>
        <ul>
            {{#each this.food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>

        <h2>Brewers Tips: </h2>
        <p>{{this.brewers_tips}}</p>
    </div>
{{/each}}