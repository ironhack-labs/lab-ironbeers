<div class="random-beer">
    <img src="{{image_url}}" alt="">
    <h1 class="name">{{name}}</h1>
    <p class="tagline">{{tagline}}</p>
    <p class="description">{{description}}</p>
    <h3>Food Pairing</h3>
    <ul>
        {{#each food_pairing}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <p class="tips"><strong>Brewer tips</strong>{{brewer_tips}}</p>
</div>