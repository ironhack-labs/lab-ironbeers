{{#each responseFromAPI as |randomBeer|}}
    <img src="{{randomBeer.image_url}}" alt="{{randomBeer.name}}">
    <h2>{{randomBeer.name}}</h2>
    <h4>{{randomBeer.tagline}}</h4>
    <p>{{randomBeer.description}}</p>
    <h3>Food pairing</h3>
    <ul>
        {{#each randomBeer.food_pairing as |food|}}
            <li>{{food}}</li>
        {{/each}}
    </ul>
    <h3>Brewers tips</h3>
    <p>{{randomBeer.brewers_tips}}</p>

{{/each}}