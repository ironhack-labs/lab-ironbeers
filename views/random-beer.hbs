<div class="container">

    <div class="row justify-content-center">

        {{#each randomBeer}}

        <div class="col-md-4">
            <div class="card mb-4 beer-card">
                {{>beerpartial}}
                <div class="card-body">
                    <p class="card-text"><b>Food Pairing: </b>{{#each food_pairing}} <li>{{this}}</li> {{/each}}</p>
                    <p class="card-text"><b>Brewers Tips: </b>{{ brewers_tips }}</p>
                </div>
            </div>
        </div>

        {{/each}}

    </div>

</div>