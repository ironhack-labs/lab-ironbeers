<div>
    {{#each randomBeer}}
        <img src={{image_url}} alt="image of beer">
        <h2>Hello my name is {{name}}</h2>
        <h3>{{tagline}}</h3>
        <p>{{description}}</p>
        <h2>Food Pairing:</h2>
        <ul>
            <li>{{food_pairing}}</li>
        </ul>
        <p>Brewery Tips: {{brewers_tips}}</p>
    {{/each}}
</div>