{{#each randombeer}}
<img id="beersImg"src="{{this.image_url}}" class="player-img" alt="player img">
<div class="conText">
<h1>{{this.name}}</h1>
<p>{{this.description}}</p>
<p>{{this.tagLine}}</p>
<h2>Food Pairing</h2>
<ul>
    {{#each this.food_pairing}}
    <li>{{this}}</li>
    {{/each}}
</ul>
<p><b>Brewers Tips: </b>{{this.brewers_tips}}</p>
</div>
{{/each}}