<article id="rand-beer">
    <div>
        <img src="{{beer.image_url}}" alt="{{beer.image}}" id="rand-img">
    </div>
    <div id="beer-info">
        <h1>{{beer.name}}</h1>
        <h2>{{beer.tagline}}</h2>
        <p>{{beer.description}}</p>
        <h3>Food Pairing</h3>
        <ul>
            {{#each beer.food_pairing}} 
            <li>{{this}}</li>
            {{/each}}
        </ul>
         <h3>Brewer Tips:</h3>
         <p>{{beer.brewers_tips}}</p>
    </div>
</article>