<div class="container">

    {{#each randomBeers}}

    <div class="randomBeer">

        <img src="{{image_url}}" class="image-random" alt="{{name}}">
        <div>
            <h5>{{name}}</h5>
            <p>{{description}}</p>
            <p>{{tagline}}</p>
            <p>{{food_pairing}}</p>
            <p>{{brewers_tips}}</p>
        </div>

    </div>

    {{/each}}
</div>