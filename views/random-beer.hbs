{{!-- image, name, description, tagline, food pairing and brewer tips. --}}

<main>
    <img src="{{this.image_url}}" alt="">
    <h3>{{this.name}}</h3>
    <span>{{this.tagline}}</span>
    <p>{{this.description}}</p>
    <ul>
        <h4>Food Pairing</h4>
        {{#each this.food_pairing}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <p><strong>Brewers tips: </strong> {{this.brewers_tips}}</p>

</main>