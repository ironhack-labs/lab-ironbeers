<div>
    {{#each responseFromAPI}}
        <img src={{this.image_url}}/>
        <h3> {{this.name}} </h2>
        <p> {{this.tagline}} </p>
        <p> {{this.description}} </p>
        <h3> Food pairing </h3>
        <ul>
            {{#each this.food_pairing}}
                <li> {{ this }} </li>
            {{/each}}
        </ul>
        <p> Brewers tips: {{ this.brewers_tips }}</p>
    {{/each}}
</div>
    