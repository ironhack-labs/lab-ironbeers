<div class="random-beer-container">
  {{#each randomBeer}}
  <div class="beer-props">
    {{> beerpartial this}}
    <h2>Food Pairing</h2>
    <ul>
      {{#each food_pairing}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
    <p class="beer-props"><strong>Brewer Tips: </strong>{{brewers_tips}}</p>
  </div>
{{/each}}
</div>


