{{#each randBeer}}
    <img src="{{image_url}}">
    <h2>{{name}}</h2>
    <blockquote>{{this.tagline}}</blockquote>
    <p>{{this.description}}</p>
    <ul>
        {{#each this.food_pairing}}
            <li>{{this}}</li>
        {{/each}}
    </ul>
    <span>
        <p><b>Brewer Tips:</b> {{this.brewers_tips}}</p>
    </span>
{{/each}}