{{#each randomBeer}}
    <div class="container">
        {{> beerpartial }}

        <h2>Food Pairing</h2>
        <ul>
            {{#each this.food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>

        <h2>Brewers Tips: </h2>
        <p>{{this.brewers_tips}}</p>
    </div>
{{/each}}