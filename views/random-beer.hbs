<h1>Random Beer</h1> 

<ul>


{{#each beer}} 
    <li>Name: {{{this.name}}}</li>
    <li>Description: {{{this.description}}}</li>
    <li>Tagline {{{this.tagline}}}</li>
    <li>Food Pairing {{{this.food_pairing}}}</li>
    <li>Brewers Tips {{{this.brewers_tips}}}</li>
    <li><img src="{{{image_url}}}" alt=""></li>

{{/each}}


</ul>
