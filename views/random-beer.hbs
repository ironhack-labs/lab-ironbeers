
<ul>
  {{#each responseFromAPI}}

    <img src='{{this.image_url}}' alt='Beers'>

    <h1>{{this.name}}</h1>

    <p>{{this.description}}</p>

    <h6>{{this.tagline}}</h6>

    <h3>Food Pairing</h3>

    {{#each food_pairing}}
    <ul>
        <li>{{this}}</li>
    </ul>
    {{/each}}
 <h5>Brewer Tips:</h5>
    <p>{{this.brewers_tips}}</p>



  {{/each}}

</ul>