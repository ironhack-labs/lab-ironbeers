{{#each randombeer}}
<div class="random-beer">
    <div>
        <img id="random-beer-img" src="{{image_url}}">
    </div>
    <div class="random-beer-content">
        {{name}}
        {{tagLine}}
        {{description}}
        Food Pairing: {{#each food_pairing}}
        <li> 
            {{this}} 
        </li>
        {{/each}}
        <p>Brewers Tips: {{brewers_tips}}</p>
    </div>
</div>
{{/each}}