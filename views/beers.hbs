<div class="row">
    {{#each beers as | beer |}}
        <div class="col-3">
            <div>
                <img class="w-25" src="{{beer.image_url}}" >
                <p>{{beer.name}}</p>
                <p>{{beer.description}}</p>
                <p>{{beer.tagline}}</p>
            </div>
        </div>
    {{/each}}
</div>