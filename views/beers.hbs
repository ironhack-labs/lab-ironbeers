<div class="container">
  <h1>Beers</h1>
   <a href="/">Home</a>
  {{#each beersFromApi}}
    <div class="beer-card">
      <img src="{{ this.image_url }}" alt="{{ this.name }}">
      <h2>{{ this.name }}</h2>
      <p>{{ this.description }}</p>
      <h3>{{ this.tagline }}</h3>
    </div>
  {{/each}}
</div>