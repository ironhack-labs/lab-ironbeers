<div class='container'>
  {{#each beers as |beer|}}
    <div class='card' style='width: 18rem;'>
      <img src='{{beer.image_url}}' class='card-img-top' alt='...' />
      <div class='card-body'>
        <h5 class='card-title'>{{beer.name}}</h5>
        <p class='card-text'>{{beer.description}}</p>
        <p class='card-text'>{{randomBeer.tagline}}</p>
      </div>
    </div>
  {{/each}}
</div>