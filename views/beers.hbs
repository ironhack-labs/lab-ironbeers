<div class="d-flex align-content-betwen flex-wrap">
    {{#each beers}}
    <div class="card" style="width: 450px; height: 700px">
        <img src="{{this.image_url}}" class="card-img-top mt-2 justify-content-center" style="height: 300px; width: 100px"alt="beers img">
        <div class="card-body">
            <h5 class="card-title">{{this.name}}</h5>
            <p class="card-text">{{this.description}}</p>
            <h6 class="card-title">{{this.tagline}}v</h6>
        </div>
    </div>
    {{/each}}
</div>