<div class='container'>
  <h1>Beers</h1>
  <div class='row'>

    {{#each beersFromApi}}
      <div class='col-md-4'>
        <div class='card'>
          <img class='card-img-top' src='{{image_url}}' alt='beer' />
          <div class='card-body'>
            <h5 class='card-title'>{{name}}</h5>
            <p class='card-text'>{{description}}</p>
          </div>
        </div>
      </div>
    {{/each}}

  </div>
</div>