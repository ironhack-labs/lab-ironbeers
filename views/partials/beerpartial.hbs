<div class="col-md-4">
  <div class="card">
    <img
      class="card-img-top"
      src={{this.image_url}}
    />
    <div class="card-body">
      <h5 class="card-title">{{this.name}}</h5>
      <h6 class="tag-line">{{this.tagline}}</h6>
      <p class="card-text">{{this.description}}</p>
    </div>
  </div>
</div>
