<div class="card">
  <img class="card-img" src="{{this.image_url}}"  alt="">
  <div class="card-body">
    <h2 class="card-title"><a href="/beers/{{ id }}">{{this.name}}</a></h2>
    <p class="card-tagline">{{this.tagline}}</p>
    <p class="card-description">{{this.description}}</p>
  </div>
</div>