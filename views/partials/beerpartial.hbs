<div class='col-md-4'>
  <div class='card'>
    <img class='card-img-top' src='{{this.image_url}}' alt='' />
    <div class='card-body'>
      <a href='/beers/beer-{{this.id}}'>
        <h5 class='card-title'>{{this.name}}</h5></a>
      <h4 class='card-title'>{{this.tagline}}</h4>
      <p class='card-text'>{{this.description}}</p>
    </div>
  </div>
</div>