<div class="col-md-4">
  <div class="card">
    <img class="card-img-top" src="{{this.image_url}}" alt="{{this.name}}"/>
    <div class="card-body">
      <h2 class="card-title">{{this.name}}</h2>
      <h4 class="card-text">{{this.description}}</h4>
      <h4 class="card-text">{{this.tagline}}</h4>
    
    </div>
  </div>
</div>