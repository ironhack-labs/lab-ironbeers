<div class="col-md-4">
    <div class="card">
        <img class="card-img-top;" src="{{this.image_url}}" alt="Card image">


        <div class="card-body">
            <h4 class="card-title">{{this.name}}</h4>
            <p class="card-text">{{this.description }}</p>
            <p class="card-text">{{this.tagline }}</p>
        </div>
    </div>
</div>