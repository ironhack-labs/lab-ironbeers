<div class="card">
    <div class="card-top" >
        <img src='{{this.image_url}}' alt="" >
    </div>
    <div class="card-body" >
        <div class="card-sprites">
            {{#each this.sprites}} 
                <img src="{{this}}"/>
            {{/each}}
        </div>
        <h5 class="beer-title">{{this.name}} <span>{{this.id}}</span></h5>
        <div class="beer-text"> {{this.description}}</div>
        <div class="beer-tag"> {{this.tagline}}</div>

    </div>
</div>