<div class="card h-100">
  <img src="{{this.image_url}}" class="card-img-top img-beer" alt="{{this.name}}">
  <div class="card-body">
    <h5 class="card-title">{{this.name}}</h5>
    <h6>{{this.tagline}}</h6>
    <p class="card-text">{{this.description}}</p>
    <p class="card-text">{{this.food_pairing}}</p>
        <br>
    <p class="card-text"><strong>Brewers tips:</strong>{{this.brewers_tips}}</p>
  </div>
</div>