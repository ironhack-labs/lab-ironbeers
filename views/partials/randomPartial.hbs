<div class="col-md-2">
    <article class="beer random">
        <img src="{{image_url}}" class="beer-img" alt="{{beer-img}}">
</div>
<div class="col-md-10">
    <h5 class="name-title">{{name}}</h5>
    <p class="tagline-text">{{tagline}}</p>
    <p class="description-text">{{description}}</p>
    <h6>Food Pairing </h6>
    <ul> {{#each food_pairing}}
        <li>{{this}}
        </li>
        {{/each}}
    </ul>
    <p> <b>Brewerer Tips:</b> {{brewers_tips}}</p>
</div>
</article>