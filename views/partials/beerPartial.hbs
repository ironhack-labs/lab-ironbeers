<article class="col-lg-4">
    <div class="card-body">
        <img class="card-img-top" src="{{image_url}}" alt="{{this.name}}" />
        <h5 class="card-title">{{this.name}}</h5>
        <p class="card-text">{{this.description}}</p>
        <p class="card-text">{{this.tagline}}</p>
    </div>
</article>