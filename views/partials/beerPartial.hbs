
<div class="card" style="width: 18rem;">
	<img src="{{this.image_url}}" class="card-img-top" alt="beer pic">
	<div class="card-body">
		<a href='beers/beer-{{this.id}}'><h5 class="card-title">{{this.name}}</h5></a>
		<p class="card-text">{{this.tagline}}</p>
		<p class="card-text">{{this.description}}</p>
	</div>
</div>

