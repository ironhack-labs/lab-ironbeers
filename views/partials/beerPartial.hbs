<div class="card col-4">
    <a href="/beers/beer-{{id}}" class="beer-link">
        <img class="card-img-top" src="{{this.image_url}}" alt="Card image cap">
        <div class="card-body">
            <h1 class="card-title">{{this.name}}</h1>
            <h2 class="card-subtitle">{{this.tagline}}</h2>
            <p class="card-text">{{this.description}}</p>
        </div>
    </a>
</div>