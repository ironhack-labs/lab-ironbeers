<ul class='container-beers'>
  {{#each beersFromApi}}
    <li>
      <img src='{{this.image_url}}' />
      <div class='container-text'>
        <h2 id='title'>{{this.name}}</h2>
        <p id='subtitle'> {{this.tagline}}</p>
        <p>{{this.description}}</p>
      </div>
    </li>
  {{/each}}
</ul>