<div class="beer-card">
    <article>
        <a href="/beer-info/{{this.id}}">
        <img src="{{this.image_url}}" alt="Beer">
        </a>
        <h3>{{this.name}}</h3>
        <p class="tagline">
           {{this.tagline}} 
        </p>
        <p class="description">
            {{this.description}}
        </p>
    </article>
</div>