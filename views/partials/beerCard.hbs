<div class='card'>
  <div class='card-body'>
    <img src='{{this.image_url}}' alt='beer' />
    <h5 class='card-title'>
      {{this.name}}
    </h5>
    <h6 class='card-subtitle mb-3 text-muted'>
      {{this.tagline}}
    </h6>
    <p class='card-text'>
      {{this.description}}
    </p>
  </div>
</div>