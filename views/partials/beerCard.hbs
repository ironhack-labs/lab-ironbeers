<div class="col-md-4 col-lg-4">
    <article class="card">
        <img class="img-beer card-img-top" src="{{this.image_url}}">
        <div class="card-body">
            <h3 class="card-title">{{this.name}}</h3>
            <p class="card-text">{{this.tagline}}</p>
            <hr>
            <p class="card-text">{{this.description}}</p>
        </div>
    </article>
</div>