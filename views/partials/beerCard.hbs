<div class="col-md-4">

    <article class="card beer-card">
        <img class="card-img-top beers-img" src="{{this.image_url}}">
        <div class="card-body">
            <h5 class="card-title">{{this.name}} {{this.lastName}}</h5>
            <hr>
            <p class="card-text tagline">{{this.tagline}}</p>
            <p class="card-text"> {{this.description}}</p>
        </div>
    </article>

</div>