
<div class="col-md-4">
  <div class="card">
    <img class="card-img-top" src="{{this.image_url}}" alt="{{this.name}}" />
    <div class="card-body">
      <h5 class="card-title">
        {{this.name}} {{this.tagline}}
      </h5>
      <p class="card-text">
        {{this.description}}
      </p>
    </div>
  </div>
</div>