<div class="col-md-4">
  <div class="cardRandom">
    <div class="containerImgRandom">
        <img class="card-img-top" src="{{this.image_url}}" alt="{{this.name}}"/>
    </div>
    <div class="card-body">  
      <h5 class="card-title">{{this.name}}</h5>
      <p class="card-text">{{this.description}}</p>
      <p class="card-text-tagline">{{this.tagline}}</p>
      <p class="card-text"><b>Food pairing:</b>{{this.food_pairing}}</p>
      <p class="card-text"><b>Brewers tips:</b>{{this.brewers_tips}}</p>
    </div>
  </div>
</div>
