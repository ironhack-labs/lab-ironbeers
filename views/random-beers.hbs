<div class="container">
    <div class="row">
        {{#each beers}}
        <div class="col-md-4">

            <div class="card mt-5 beer-card">
                <img src="{{image_url}}" class="card-img-top">
                <div class="card-body">
                    <h5 class="card-title">{{name}} {{description}}</h5>
                    <p class="card-text">{{tagline}}</p>
                </div>
            </div>
        </div>
        {{/each}}


    </div>

</div>