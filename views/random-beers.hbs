<div class="container">
    {{#each randomBeers}}
    <img src="{{image_url}}" alt="{{name}}">
    <h1>{{name}}</h1>
    <h2>{{tagline}}</h2>
    <p>{{description}}</p>
    <h3>Food Pairing</h3>
    <ul>
        {{#each food_pairing}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <h3>Brewer Tips</h3>
    <p>{{brewers_tips}}</p>
    {{/each}}
</div>