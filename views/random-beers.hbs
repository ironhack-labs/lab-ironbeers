{{#each randomBeer}}
    <div class="beer-card">
        {{> beerpartial}}
        <h3>Food pairing:</h3>
        <ul class="food-pairing">
            {{#each food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <b>Brewer's tips: </b><p>{{this.brewers_tips}}</p>
    </div>
{{/each}}