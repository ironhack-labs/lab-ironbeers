 <div id="random-beer-content">
        <img id="random-beer-img" src="{{beer.image_url}}" alt="{{beer.name}} image" >
        <div><h2>{{beer.name}}</h2>
 
        <h4>{{beer.tagline}}</h4>

        <p>{{beer.description}}</p>

        <h4>Food Pairing</h4>
        <ol>
        {{#each beer.food_pairing}}
            <li>{{this}}</li>
        {{/each}}
        </ol>

        <h4>Brewer tips: <span>{{beer.brewers_tips}}</span></h4>
        </div>
</div>