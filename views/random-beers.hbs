<div class='container-random-beers'>

  {{#each beers}}
    <div class='card-random'>
      <h1>{{this.name}}</h1>
      <p>{{this.tagline}}</p>
      <picture><img src='{{this.image_url}}' alt='' /></picture>
      <p>{{this.description}}</p>
      <p>{{this.food_pairing}}</p>
      <p>{{this.brewers_tips}}</p>
    </div>
  {{/each}}
</div>