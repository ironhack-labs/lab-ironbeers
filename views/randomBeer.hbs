<div class="randomBeer">
    <div class="randomImage">
        {{#each randomBeer}}
        <img src="{{image_url}}">
    </div>
    <div class="randomText">
        <h1>{{name}}</h1>
        <p>{{description}}</p>
        <p>{{tagline}}</p>
        <p>{{food_pairing}}</p>
        <p>{{brewers_tips}}</p>
        {{/each}}
    </div>
</div>