<div class="randomBeer">
{{#each beer}}
    <img src="{{this.image_url}}" alt="picture of {{this.name}}">
    <h3>{{this.name}}</h3>
    <h4 class="tagline">{{this.tagline}}</h4>   
    <p>{{this.description}}</p>
    <h4>Food Pairing:</h4>
    <ul>
        {{#each this.food_pairing}}
            <li>
                {{this}}
            </li>
        {{/each}}
    </ul>
    <h4>Brewers Tips:</h4>
      <p>{{this.brewers_tips}}</p>  
{{/each}}
</div>