    <div>
        {{#each this}}
            {{#if @first }}
                {{> beerpartial}}
                <h5>Food Pairing</h5>
                <ul>
                    {{#each this.food_pairing}}
                    <li>{{this}}</li>
                    {{/each}}
                </ul>
                <p><strong>Brewer Tips:</strong>{{this.brewers_tips}}</p>
            {{/if}}
        {{/each}}
    </div>


