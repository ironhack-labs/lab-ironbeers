<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>Document</title>
  </head>
  <body>
    {{#each randomBeer}}
      <div class='random-beer'>
        <img src='{{image_url}}' alt='' />
        <div class='random-beer-description'>
          <h1>{{name}}</h1>
          <h2>{{tagline}}</h2>
          <p>{{description}}</p>
          <p><span>Food Pairing</span></p>
          <ul>
            {{#each food_pairing}}
              <li>
                {{this}}
              </li>
            {{/each}}

          </ul>
          <p><span>Brewers Tips: </span>{{brewers_tips}}</p>
        </div>

      </div>
    {{/each}}
  </body>
</html>