<div class="random-beer">
    <div class="random-beer-img">
    <img class="beer-img-top" src="{{new.image_url}}" alt="" />

    </div>
    <div class="random-beer-info">
        <h4>{{new.name}}</h4>
        <p>{{new.description}}</p>
        <p>{{new.tag_line}}</p>
        <h5>Food Pairing:</h5>
        <div class="pairing-list">
            <ul>
                {{#each new.food_pairing}}
                <li>{{this}}</li>
                {{/each}}
            </ul>
        </div>
        <p><span>Brewer tips: </span>{{new.brewers_tips}}</p>
       

    </div>
</div>