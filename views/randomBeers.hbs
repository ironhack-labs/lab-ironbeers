<section class="randomBeers">

    <div class="image"> <img src="{{image_url}}" alt="randomBeerImg"></div>

    <div class="name">{{name}}</div>
    <div class="description">{{description}}</div>
    <div class="tagline">{{tagline}}</div>
    <div class="food-pairing">
        <h4>Food Pairings:</h4>
        <ul>
            {{#each food_pairing}}
    <li>
        {{this}}    
    </li>
            {{/each}}
        </ul>
        </div>
    <div class="brewer-tips">{{brewers_tips}}</div>
</section>