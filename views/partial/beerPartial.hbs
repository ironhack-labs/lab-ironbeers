<div class='col-md-2'>
  <div class='card' data-href='/beers/beer-{{this.id}}'>
    <img class='card-img-top' src='{{this.image_url}}' alt='{{this.name}}' />
    <div class='card-body'>
      <h5 class='card-title'>{{this.name}}
        {{this.description}}</h5>
      <p class='card-text'>{{this.tagline}}</p>
    </div>
  </div>
</div>