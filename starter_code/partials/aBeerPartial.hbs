<div class="col-md-3"><img src="{{this.image_url}}"></div>
<div class="col-md-9">
  <h2 class="beer-title">{{this.name}}</h2>
  <div class="beer-tagline">{{this.tagline}}</div>
  <div class="beer-description">
    <p>{{this.description}}</p>
  </div>
  <div class="food-pairing">
    <h3>Food Pairing</h3>
    <ul>
      {{#each this.food_pairing }}
      <li>{{this}} </li>
      {{/each}}
    </ul>
  </div>
  <div class="brewer-tips">
    <p><strong>Brewers Tips</strong> {{this.brewers_tips}}</p>
  </div>
</div>

{{log this}}