<h1>Random Beers</h1>
<ul class="beers">
    
<li><img src={{beer.image_url}}>
<div class="random-beer">
    <h1>{{beer.name}}</h1>
    <h3>{{beer.tagline}}</h3>
    <p>{{beer.description}}</p>
    <h4>Food Pairing</h4>
    {{#each beer.food_pairing}}
        <li>{{this}}</li>
    {{/each}}
    <h4>{{brewers_tips}}</h4>
</div>
   
</ul>