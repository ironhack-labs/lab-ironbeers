{{#each beer}}
<div class="random">
      <div class="col-md-8">
            <h1 class="display-5 font-weight-normal">{{this.name}}</h1>

            <h3 class="font-weight-normal">{{this.tagline}}</h3>
            <p class="lead ">{{this.description}}</p>
            <p class="lead font-weight-small"><b>Food pairing:</b> </p>
            <ul>
                  {{#each this.food_pairing}}
                  <li> {{this}}</li>
                  {{/each }}
            </ul>
            <p class="lead font-weight-small "><b>Brewer tips:</b> {{this.brewers_tips}}</p>
      </div>
      <div class="">
            <img src="{{this.image_url}}" class="random-img">
      </div>

</div>
{{/each }}