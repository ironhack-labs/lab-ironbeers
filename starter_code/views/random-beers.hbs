<div class="container-one">
   {{#each beers}}
   <div class="beerDescriptionSingle">
       <img class="beerImg" src="{{this.image_url}}" alt=“”>
       <p class="beername"> {{this.name}} </p>
       <p class="smallLetter">{{this.tagline}}</p>
       <p class="description">{{this.description}}</p>
       <h3>Food Pairing</h3>
       {{#each this.food_pairing}}
       <li>{{this}}</li>
       {{/each}}
       </ul>
       <p class="tips"> <span> Brewer Tips</span> : {{this.brewers_tips}}</p>
   </div>
   {{/each}}
</div>
   

