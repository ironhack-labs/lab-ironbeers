<div class="container"> 
  <div class="row p-5 m-5">
    <div class="col">
      <h3 class="text-uppercase">{{ randomBeer.name }}</h3>
      <p> - {{ randomBeer.tagline }} </p>
      <p> {{ randomBeer.description }} </p>
      
      
      <ul>
      {{#each randomBeer.food_pairing}} 
      <li> {{this}}</li>
      {{/each}} 
      </ul>
      
    </div>
    <div class="col d-flex justify-content-center">
      <img src="{{ randomBeer.image_url }}" alt="" width="100px" height="300px">
    </div>
  </div>
    <hr>
</div>