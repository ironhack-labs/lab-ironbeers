<h1> A RANDOM - BEER</h1>
{{#each beers}}
<div class="Beer" style="width: 33.333%;">
  <img class="beer-img-top" src="{{this.image_url}}" alt="A special beer take randomly">
  <div class="beer-body">
    <h2 class="beer-name"> {{this.name}}</h2>
    <h4 class="beer-title">{{this.tagline}}</h4>
    <p class="beer-text">{{this.description}}</p>
  </div>
</div>
{{/each}}  