<div class="container" id="random-beer-container">
  <div class="row">
   <div class="col-md-3">
    <img src="{{image_url}}" height="300">
  </div>
  <div class="col-md-9">
    <h5>{{name}}</h5>
    <h6>{{tagline}}</h6>
    <p>{{description}}</p>
    <ul>
      <strong>Food Pairing</strong>
      {{#each food_pairing}}
      <li>{{this}}</li>
      {{/each}}</ul>
      <p><strong>Brewers Tips:</strong>{{brewers_tips}}</p>
  </div>
  </div>
</div>