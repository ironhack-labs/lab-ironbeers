<div class="all-content">
{{#each beers}} 
<div class="beer-content">
<img class="beers-img" src="{{image_url}}" alt="beer-pic">
</div>
<div class="description">
<h3>{{name}}</h3>
<p>{{tagline}}</p>
<p class="description-text">{{description}}</p>

<h4>Food Pairing</h4>
<ul>
{{#each food_pairing}} 
  <li>{{this}}</li>
{{/each}}
</ul>
<p><strong>Brewers Tips: </strong>{{brewers_tips}}</p>
</div>
{{/each}}
</div>
