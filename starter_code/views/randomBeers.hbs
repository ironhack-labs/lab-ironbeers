
{{#each beers}}
<div class="card">
    <div class="img-beers2">
      <img class="card-img-top" src="{{this.image_url}}" alt="beers">
    </div>
      <div class="card-body">
        <h4 class="card-title">{{this.name}}</h4>
        <p class="card-text">{{this.tagline}}</p>
        <p class="card-text">{{this.description}}</p>
        <h5>Food Pairing</h5>
        <ul>
          {{#each this.food_pairing}}
            <li>{{this}}</li>
          {{/each}}
        </ul>
        <p><strong>Brewers tips: </strong>{{this.brewers_tips}}</p>
      </div>
</div>
{{/each}}