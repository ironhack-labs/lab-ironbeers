<div class="single-beer">
  <img class="each-beer-img" src="{{ image_url }}" alt="{{ name }} picture">
  <div class="random-description">
    <h1>{{ name }}</h1>
    <h2>{{ tagline }}</h2>
    <p>{{ description }}</p>
    <h3>Food Pairing:</h3>
    <ul>
      {{#each food_pairing}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
    <p><b>Brewers Tips:</b>{{ brewers_tips }}</p>
  </div>
</div>