<div id="rand">
  <div class="random-image-container">
    <div id="random-image">
      <img src="{{beer.image_url}}" alt="">
    </div>
    <div id="random-text"=>
      <h2>{{beer.name}}</h2>
      <p class="grey-p">{{beer.tagline}}</p>
      <p>{{beer.description}}</p>

      <h3>Food pairing</h3>
      {{#each beer.food_pairing}}
      <li>{{this}}</li>
      {{/each}}

      <strong>Brewer Tips:</strong> {{beer.brewers_tips}}
    </div>
  </div>
</div>