<div>
    {{#each beer}}
    <img src="{{this.image_url}}" alt="beer-photo">
    <h2>{{this.name}}</h2>
    <p>{{this.description}}</p>
    <p>{{this.tagline}}</p>
    <h2>Food Pairing</h2>
    <ul>
        {{#each this.food_pairing}}
        <li>{{ this }}</li>
        {{/each}}
    </ul>
    <p><b>Brewers Tips:</b> {{this.brewers_tips}}</p>
    {{/each}}
</div>