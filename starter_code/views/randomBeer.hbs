<div class="row">
  <img id="randBeer" class="" src="{{this.image_url}}">
  <div id="randBeerExplain" class="col-8">
    <h1>{{this.name}}</h1>
    <quote>{{this.tagline}}</quote>
    <p>{{this.description}}</p>
    <h1>Food Paring</h1>
    <ul>
      {{#each this.food_pairing}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
    <p><strong>Brewers Tip:</strong> {{this.brewers_tips}}</p>
  </div>
</div>
