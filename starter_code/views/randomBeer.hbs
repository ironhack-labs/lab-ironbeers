<div class="cards-container">
  {{#each beer}}
<div class="card" style="width: 33.333%;">
      <img class="card-img-top" src="{{this.image_url}}" alt="">
      <div class="card-body">
        <h5 class="card-title">{{this.name}}</h5>
        <h6 class="card-text" id="grey">{{this.tagline}}</h6>
        <p class="card-text">{{this.description}}</p>
        <h5 class="card-title">Food Pairing</h5>
        {{#each food_pairing}}
        <li>{{this}}</li>
        {{/each}} <br>
        <h5 class="card-title">Brewers Tips:</h5>
        <p class="card-text">{{brewers_tips}}</p>



        
      </div>
</div>
      {{!-- {{>beerPartial this }} --}}
  {{/each}}
</div>
