<div class="beer-container">
    {{!-- {{#each beer}} --}}
    {{> beerPartial beer}}
    {{!-- {{/each}} --}}
    <h3>Food pairings</h3>
    <ul>
        {{#each beer.food_pairing}}
        <li>
            {{this}}
        </li>

        {{/each}}
    </ul>
    <p><h3>Brewers tips:</h3>{{beer.brewers_tips}}</p>
    
</div>