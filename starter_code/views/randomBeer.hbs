<div class="random-beer-container">
    {{#each beer}}
    <div>
        {{> beerpartial this}}
    </div>
    <div>
        <h2>Food Pairing</h2>
        <ul>
            {{#each this.food_pairing}}
            <li>{{ this }}</li>
            {{/each}}
        </ul>
        <p><b>Brewers Tips:</b> {{this.brewers_tips}}</p>
    </div>
    {{/each}}
</div> 
