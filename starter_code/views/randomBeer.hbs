{{#each randombeer}}
  <div class="container mx-auto p-5 row">

    <div class="col-2">
      <img class="img-fluid" src="{{this.image_url}}" alt="">
    </div>
    <div class="col-10">
      <h3>{{this.name}}</h3>
      <p class="text-muted">{{this.tagline}}</p>
      <p>{{this.description}}</p>
      <h5>Food Pairing</h5>
      <ul>
        {{#each this.food_pairing}}
          <li>{{this}}</li>
        {{/each}}
      </ul>
      <p><strong>Brewers Tips: </strong>{{this.brewers_tips}}</p>
    </div>

  </div>
{{/each}}
