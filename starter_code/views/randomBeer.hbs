<div>
  {{#each beers}}
    <img src="{{this.image_url}}" alt="{{this.name}}">
    <h2> {{this.name}}</h2>
    <h3>{{this.tagline}}</h3>
    <p>{{this.description}}</p>
    <h4>Food Pairing</h4>
    <ul>
      {{#each this.food_pairing}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
    <p>Brewers Tips:{{this.brewers_tips}}</p>
  {{/each}}
</div>