<div class="random">
  <div class="imager"><img class="beer-img-top" src="{{this.image_url}}" alt="Beer Image"></div>
  <div class="beer-body">
    <h4 class="beer-title">{{this.name}}</h5>
    <h5 class="beer-tag">{{this.tagline}}</h5>
    <p class="beer-des">{{this.description}}</p>
    <h5>Food Pairing</h5>
    <ul>
      {{#each this.food_pairing}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
    <p><b>Brewer Tips: </b>{{this.brewers_tips}}</p>
  </div>
</div>