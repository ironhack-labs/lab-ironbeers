{{#each beers}}
    <div class="randomPartial">

            <img src="{{image_url}}" alt"">

        <div class="description">
            <h3>{{name}}</h3>
            <h5>{{tagline}}</h5>
            <p>{{description}}</p>

            <h4>Food Pairing</h4>

            {{#each food_pairing}}
                <li>{{this}}</li>
            {{/each}}

            <p><b>Brewers Tips:</b> {{brewers_tips}}</p>
        </div>
    </div>
{{/each}}
