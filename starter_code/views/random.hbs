
<div class="beers">
    <div class="row">
        {{#each beer}}
        <div class="col-md-4">
            <img class="card-img-top" src="{{this.image_url}}" alt="">
            <h3 class="card-title">{{this.name}}</h3>
            <h5 class="card-subtittle">{{this.tagline}}</h5>
            <p class="card-text">{{this.description}}</p>
        </div>
        {{/each}}
    </div>
</div>