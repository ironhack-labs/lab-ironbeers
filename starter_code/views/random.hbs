<div class="main">
    <div class="randbeerimg">
  <img class="rand-img" src={{image_url}} alt="A Beer">
    </div>
  <div class="rand-body">
    <h5 class="rand-title">{{name}}</h5>
    <p class="rand-text">{{description}}</p>
    <h2>Food Pairing</h2>
    <ul>
        {{#each food_pairing }}
        <li class = "item-rand">{{this}}</li>
        <br>
        {{/each}}
    </ul>
    <div class="brew-tips">
    <p><span>Brewer Tips:</span> {{brewers_tips}}</p>
    </div>



  </div>
</div>