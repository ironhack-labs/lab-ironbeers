<div class="randomCard">
    <div class = "randomImg">
    <img class="card-img-top" src="{{this.image_url}}" alt="Card image cap">
    </div>
    <div class = "randomInfo">
        <h1 >{{name}} </h1>
        <h6 class = "card-tag">{{tagline}}</h6>
        <p class="card-text">{{description}}</p>
        <h4>Food-Pairing:</h4>
        {{#each food_pairing}}
        <li>{{this}}</li>
        {{/each}}
        <h4>Brewers tips:</h4><p>{{brewers_tips}}</p>
    </div>
</div>