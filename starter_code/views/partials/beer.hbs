<div class="beer-card">
      <img class="beer-img" src="{{this.image_url}}" alt="Card image cap">
      <div class="beer-info">
        <h5 class="beer-name">{{this.name}}</h5>
        <p class="beer-tagline">{{this.tagline}}</p>
        <p class="beer-description">{{this.description}}</p>
      </div>
</div>