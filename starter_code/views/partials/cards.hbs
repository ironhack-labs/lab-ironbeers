<div class="card">
  <img class="card-img-top" src="{{this.image_url}}" alt="Card image cap">
  <div class="card-body">
    <h4 class="card-title">{{this.name}}</h4>
    <p class="tagline-text">
        {{this.tagline}}
    </p>
    <p class="card-text">
        
        {{this.description}}
     
    </p>
    <a href="#!" class="btn btn-primary">Go somewhere</a>
  </div>
</div>