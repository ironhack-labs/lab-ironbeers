<div class="card">
  <div class="my-card-img">
      <img class="card-img-top" src="{{this.image_url}}" alt="Card image cap"> 
  </div>
  <div class="card-body">
    <h4 class="card-title">{{this.name}}</h4>
    <h4 class="card-text">{{this.tagline}}</h4>
    <p class="card-text">
      {{this.description}}
    </p>
  </div>
</div>