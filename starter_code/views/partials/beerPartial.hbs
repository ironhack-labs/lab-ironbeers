<!-- beerPartial.hbs -->

<div class="card col-md-4">
  <img class="card-img-top" src="{{this.image_url}}" alt="Beer bottle">
  <div class="card-body">
    <h5 class="card-title">{{this.name}}</h5>
    <h6 class="card-title">{{this.tagline}}</h6>  
    <p class="card-text">{{this.description}}</p>
  </div>
</div>