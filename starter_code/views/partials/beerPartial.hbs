<div class="cards-container">
<div class="card">
      <div class="card-img-div">
        <img src="{{this.image_url}}" alt="Card image cap">
      </div>
      <div class="card-body">
        <h3 class="card-title">{{this.name}}</h3>
        <h6> {{this.tagline}} </h6>
        <p class="card-text">{{this.description}}</p>
      </div>
</div>
</div>