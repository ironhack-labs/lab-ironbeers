<div class="card-beer col-md-4" >
  <div class="beer-img text-center"">
    <img src="{{this.image_url}}" alt="{{this.name}}">
  </div>

  <div class="card-body">
    <h3 class="card-title">{{this.name}}</h3>
    <p class="card-tagline">{{this.tagline}}</p>
    <p class="card-text">{{this.description}}</p>
  </div>
</div>