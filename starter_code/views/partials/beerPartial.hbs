{{#with this }}
<div class="card beer-card" style="width: 18rem;">
  <img src="{{image_url}}" class="card-img-top" alt="{{name}}">
  <div class="card-body">
    <h5 class="card-title">{{name}}</h5>
    <p class="text-secondary"><small>{{tagline}}</small></p>
    <p class="card-text">{{description}}
    </p>

  </div>
</div>
{{/with}}