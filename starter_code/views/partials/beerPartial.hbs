<div class="card" style="width: 33.333%;">
      <img class="card-img-top" src="{{this.image_url}}" alt="beer image">
      <div class="card-body">
        <h4 class="card-title">{{this.name}}</h4>
        <h6 class="card-title">{{this.tagline}}</h6>
        <p class="card-text">{{this.description}}</p>
      </div>
</div>