<div class="card card-size">
    <img src="{{this.image_url}}" class="card-img-top" alt="Image of {{this.name}} beer bottle">
    <div class="card-body">

        <h5 class="card-title">{{this.name}}</h5>
        <h6 class="tagline">{{this.tagline}}</h6>
        <p class="card-text">{{this.description}}</p>
        <p class="card-text">{{this.food_pairing}}</p>
        <p class="card-text">{{this.brewer_tips}}</p>

    </div>
</div>