<div class="card w-50 py-5 border-0">
    <img class="card-beer-image" src="{{this.image_url}}" alt="Card image cap">
  <div class="card-body">
    <h5 class="card-title">{{this.name}}</h5>
    <h6 class="card-subtitle mb-2 text-muted">{{this.tagline}}</h6>
    <p class="card-text">{{this.description}}</p>
  </div>
</div>






