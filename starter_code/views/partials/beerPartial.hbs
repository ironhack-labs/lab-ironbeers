<div class="card card-beer col-3" style="width: 30%;">
    <img class="card-img-top" src="{{this.image_url}}" alt="">
    <div class="card-body">
        <h5 class="card-title">{{this.name}}</h5>
        <p class="card-text"><small class="text-muted">{{this.tagline}}</small></p>
        <p class="card-text">{{this.description}}</p>
    </div>
</div>