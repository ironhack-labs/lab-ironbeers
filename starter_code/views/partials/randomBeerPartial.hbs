<div class="randomBeerContainer">

     <img src="{{this.image_url}}">

      <div class="textCont">
       <h1>{{this.name}}</h1>
        <h3>{{this.tagline}}</h3>
        <p>{{this.description}}</p>
        <span>Food Pairing</span>
        <ul> 
            {{#each this.food_pairing}}
              <li>{{this}}</li>     
            {{/each}}
          </ul>
        <span><b>Brewer Tips: </b>{{this.brewers_tips}}</span>
      </div>
      <div class="clearfix"></div>
</div>