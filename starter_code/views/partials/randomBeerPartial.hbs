<div class="randomB">
      <img src="{{this.image_url}}">
      <div id="randomB">
          <h1>{{this.name}}</h1>
          <h3>{{this.tagline}}</h3>
          <p>{{this.description}}</p>
          <br>
          
          <span><b>Food Pairing</b> </span>
          <ul>
            {{#each this.food_pairing}}
            <li>
              {{this}}
            </li>
            {{/each}}
          </ul>
          <span id="tips"><b>Brewer Tips: </b>{{this.brewers_tips}}</span>

      </div>
      
</div>