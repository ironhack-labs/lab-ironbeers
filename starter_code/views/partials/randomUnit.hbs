<img src="{{image_url}}">
<div class="beer-random-description">
    <h2>{{name}}</h2>
    <h3>{{tagline}}</h3>
    <p>{{description}}</p>
    <h4>Food Pairing</h4>
    <ul>
        {{#each food_pairing}}
            <li>{{this}}</li>
        {{/each}}
    </ul>
    
    <p><b>Brewers Tips: </b>{{brewers_tips}}</p>
</div>