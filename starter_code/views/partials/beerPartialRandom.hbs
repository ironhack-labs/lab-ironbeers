<div class="card mb-3" style="width: 1200px;">
  <div class="row no-gutters">
    <div class="col-md-4">
      <img src="{{this.image_url}}" class="card-img" alt="...">
    </div>
    <div class="col-md-8">
      <div class="card-body">
        <h3 class="card-title">{{this.name}}</h3>
        <h6 class="card-title" style="color:#bfbfbf;">{{this.tagline}}</h6>
        <p class="card-text">{{this.description}}</p>
        <p class="p">Food Pairing</p>
        <ul>
          {{#each beers}}
          <li>{{this.food_pairing}}</li>
          {{/each}}
        </ul>
        <p class="p">Brewers Tips:</p> {{this.brewers_tips}}
      </div>
    </div>
  </div>
</div>