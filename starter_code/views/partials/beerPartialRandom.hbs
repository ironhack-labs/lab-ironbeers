<div id="beerinforandom">
    <img src="{{this.image_url}}" height="450px">
    <div id="randomtext">
        <h2>{{this.name}}</h2>
        <h3>{{this.tagline}}</h3>
        <p>{{this.description}}</p>

        <h4>Food Pairing</h4>
        <ul>{{#each this.food_pairing}}<li>{{this}}</li>{{/each}}</ul>
        <p><b>Brewers Tips: </b>{{brewers_tips}}</p>
    </div>
</div>