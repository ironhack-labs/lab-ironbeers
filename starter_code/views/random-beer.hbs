<div class="card" style="width: 33.333%;">
    <img class="card-img-top" src="{{this.image_url}}" alt="Card image cap">
    <div class="card-body">
        <h2 class="card-title">{{this.name}} </h2>
        <h4 class="card-title">{{this.tagline}} </h4>
        <p class="card-title">{{this.description}} </p>
        <p class="card-title">{{this.food_pairing}} </p>
        <ul>
            {{#each this.food_pairing}}
            <li> {{this}}</li>
            {{/each}}
        </ul>
        <p class="card-title">{{this.brewers_tips}} </p>
    </div>
</div>