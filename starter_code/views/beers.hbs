<div class="body-container">

  <div class="beer-container">

    {{#each beers}}
    <div class="card" style="width: 18rem;">
      <img class="card-img-top" src="{{image_url}}" alt="Card image cap">
      <div class="card-body">
        <h5 class="card-title">{{name}}</h5>
        <p class="card-text">{{description}}</p>
      </div>
    </div>
    {{/each}}
  </div>
</div>