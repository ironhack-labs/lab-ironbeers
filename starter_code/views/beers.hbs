
<ul class="theBeerList">
        {{#each beersList}}
            <li class="item">
                <img src="{{this.image_url}}" alt="">
                <h4>{{this.name}}</h4>
                <p class="tagline">{{this.tagline}}</p>
                <p>{{this.description}}</p>
            </li>
        {{/each}}
    </ul>