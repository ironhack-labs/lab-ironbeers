<div class="card" style="width: 33.333%;">
    <img class="card-img-top" src="{{this.image_url}}" alt="Card image cap">
</div>

<div class="card-body">
    <a href="/beers/{{this.id}}">
        <h2 class="card-title">{{this.name}}</h2>
    </a>
    <h3>{{this.tagline}}</h3>
    <p class="card-text">{{this.description}}</p>
</div>