<div class="card" style="width: 33.333%;">
    <img class="card-img-top" src="{{ this.image_url }}" alt="">
    <div class="card-body">
        <h5 class="card-title">{{this.name}}</h5>
        <p class="card-text">{{ this.description }}</p>
        <p>Tagline: {{ this.tagline }}</p>
        <p>{{ this.food_pairing}}</p>
        <p>{{ this.brewers_tips}}</p>
    </div>
</div>