<!-- beersCard.hbs -->
<div class="card-group">
  <div class="card">
    <a href="/beers/{{id}}"><img class="card-img-top" src="{{this.image_url}}" alt="Card image cap"></a>
    <div class="card-body">
      <a href="/beers/{{id}}"></a><h5 class="card-title">{{this.name}}</h5></a>
      <p class="card-text">{{this.description}}</p>
      <p class="card-text"><small class="text-muted">{{this.tagline}}</small></p>
    </div>
  </div>
</div>