<div>
    {{#each beer}}
    <div>
        <img src = {{this.image_url}} alt="random-beer">
        <h3>Name: {{this.name}}</h3>
        <h4>Tagline: {{this.tagline}}</h4>
        <h4>Description: {{this.description}}</h4>
        <ul>
         {{#each this.food_pairing}}
         <li>{{this}}</li>
         {{/each}}
        </ul>   
        <p>Brewers tips: {{this.brewers_tips}}</p>
    </div>
    {{/each}}
</div> 