{{#each responseFromAPI}}
    <div class="container-random">
        <div class="image-random">
            <img class= "beer-random" src={{this.image_url}}/>        
        </div>
        <div class="text-random">
            <h1 class="title-random">{{this.name}}</h1>
            <p class="tagline-random">{{this.tagline}}</p>
            <p class="description-random">{{this.description}}</p>
            <p class="food-pairing-random">{{this.food_pairing}}</p>
            <p class="tips-random">{{this.brewers_tips}}</p>
        </div>
</div>
    
{{/each}}