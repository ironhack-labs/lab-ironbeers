<section class="page">
    {{#each beer}}
    <div class="beer">
        <img class="img" src="{{this.image_url}}" alt="{{this.name}}">
        <h3>{{this.name}}</h3>
        <h4>{{this.tagline}}</h4>
        <p>{{this.description}}</p>
        <ul>
            {{#each this.food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <p>{{this.brewers_tips}}</p>
    </div>
    {{/each}}
</section>