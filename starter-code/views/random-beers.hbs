<div>
    {{#each beersFromApi}}
    {{> beerCard this}}
    <div class="card-body">
        <h3>Food Pairing</h3>
        <ul>
            {{#each this.food_pairing}}
            <li>{{this}} </li>
            {{/each}}
        </ul>
        <p><strong>Brewers Tips:</strong> {{this.brewers_tips}}</p>
    </div>
    {{/each}}
</div>