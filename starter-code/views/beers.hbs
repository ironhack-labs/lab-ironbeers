<div class="container">
{{#each theBeers}}
    <div class="container-per-beer">

        <div class="image-container">
            <img src="{{this.image_url}}" alt="{{this.name}}">
        </div>
        <div class= "name">{{this.name}}</div>
        <div class= "description">{{this.description}}</div>
        <div class= "tageline">{{this.tagline}}</div>
    </div>
{{/each}}
</div>  

