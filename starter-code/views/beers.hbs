<div class="card-columns padding bg">
    {{#each beers}}
  <div class="card">
     <img src="{{this.image_url}}" class="card-img-top product" alt="...">
       <div class="card-body">
         <h5 class="card-title">{{this.name}}</h5>
          <p class="card-text">{{this.description}}</p>
         <p class="card-text"><small class="text-muted">{{this.tagline}}</small></p>
    </div>
  </div>
  {{/each}}
</div>
