{
  "name": "lab-ironbeers",
  "version": "2.0.0",
  "license": "UNLICENSED",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js -e .js,.hbs",
    "test": "jest --forceExit",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "express": "^4.17.1",
    "hbs": "^4.1.1",
    "punkapi-javascript-wrapper": "^1.0.2"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.5",
    "jest": "^26.6.3",
    "jest-html-reporter": "^3.3.0",
    "jest-junit": "^12.0.0",
    "supertest": "^6.1.3"
  },
  "jest": {
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Lab Solution",
          "outputPath": "lab-solution.html"
        }
      ]
    ]
  },
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  }
}
